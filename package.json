{
  "name": "@curi-bio/mantarray-frontend-components",
  "version": "0.1.9",
  "description": "shared components between web and desktop app.",
  "main": "./dist/mantarray.common.js",
  "author": "Curi Bio <contact@curibio.com> (http://www.curibio.com/)",
  "repository": {
    "type": "git",
    "url": "https://github.com/CuriBio/mantarray-frontend-components.git"
  },
  "scripts": {
    "dev": "node ./node_modules/@curi-bio/frontend-test-utils/src/check_engines.js && cross-env-shell NODE_ENV=development nuxt dev --spa",
    "build-dev": "node ./node_modules/@curi-bio/frontend-test-utils/src/check_engines.js && npm run build && cross-env-shell NODE_ENV=development nuxt dev --spa",
    "build": "node ./node_modules/@curi-bio/frontend-test-utils/src/check_engines.js && vue-cli-service build --target lib --name mantarray ./index.js",
    "test:unit-no-prebuild": "node ./node_modules/@curi-bio/frontend-test-utils/src/check_engines.js && jest -c jest.config.js",
    "test:unit": "npm run build && jest -c jest.config.js",
    "test:e2e": "npm run build && npm run _test:e2e -- --app 'npm run dev' --app-init-delay 35000",
    "test:e2e-parallel-no-prebuild": "npm run _test:e2e-parallel -- --app 'npm run dev' --app-init-delay 35000",
    "test:e2e-no-prebuild": "node ./node_modules/@curi-bio/frontend-test-utils/src/check_engines.js && npm run _test:e2e",
    "test:e2e-no-prebuild-codebuild": "node ./node_modules/@curi-bio/frontend-test-utils/src/check_engines.js && npm run _test:e2e -- --app 'npm run dev' --app-init-delay 35000",
    "test:e2e-no-prebuild-windows-github": "npm run _test:e2e",
    "_test:e2e": "testcafe \"chrome:headless:emulation:width=1920;height=930;cdpPort=9223;\" ./tests/e2e --screenshots path=./tests/e2e/screenshots",
    "_test:e2e-parallel": "testcafe -c 2 \"chrome:headless:emulation:width=1920;height=930;cdpPort=9223;\" ./tests/e2e --screenshots path=./tests/e2e/screenshots",
    "prepare": "pip install -r requirements-dev.txt && pre-commit install && npx npm-merge-driver install",
    "nuxt-start": "node ./node_modules/@curi-bio/frontend-test-utils/src/check_engines.js && nuxt start",
    "lint": "eslint .",
    "docs": "jsdoc -c jsdoc.json"
  },
  "engines": {
    "node": ">=12.13.0"
  },
  "files": [
    "dist/*",
    "*.json"
  ],
  "dependencies": {
    "@fortawesome/vue-fontawesome": "^2.0.0",
    "@tofandel/uuid-base62": "^1.0.3",
    "axios": "^0.21.1",
    "base-x": "^3.0.8",
    "bootstrap": "^4.4.1",
    "bootstrap-vue": "^2.18.1",
    "d3": "^6.2.0",
    "jquery": "^3.5.1",
    "typeface-anonymous-pro": "^1.1.13",
    "typeface-muli": "^1.1.3",
    "vue-axios": "^2.1.5",
    "vue-fontawesome-icon": "^1.3.0",
    "vue-uuid": "^2.0.2",
    "vuex": "^3.5.1"
  },
  "devDependencies": {
    "@babel/core": "~7.12.3",
    "@babel/preset-env": "~7.12.7",
    "@babel/runtime-corejs2": "~7.9.2",
    "@curi-bio/frontend-test-utils": "~0.1.1",
    "@fortawesome/fontawesome-free": "~5.15.1",
    "@fortawesome/fontawesome-svg-core": "~1.2.27",
    "@fortawesome/free-brands-svg-icons": "~5.15.1",
    "@fortawesome/free-regular-svg-icons": "~5.15.1",
    "@fortawesome/free-solid-svg-icons": "~5.15.1",
    "@platform-os/testcafe-helpers": "~2.3.2",
    "@vue/cli-service": "^4.4.6",
    "@vue/test-utils": "^1.1.1",
    "autoprefixer": "~9.8.6",
    "axios-mock-adapter": "^1.18.1",
    "babel-core": "~7.0.0-bridge.0",
    "babel-eslint": "10.1.0",
    "babel-helper-vue-jsx-merge-props": "~2.0.3",
    "babel-jest": "~26.6.1",
    "babel-loader": "~8.2.2",
    "babel-plugin-syntax-jsx": "~6.18.0",
    "babel-plugin-transform-runtime": "~6.23.0",
    "babel-plugin-transform-vue-jsx": "~3.7.0",
    "babel-preset-env": "~1.7.0",
    "babel-preset-stage-2": "~6.24.1",
    "core-js": "~3.8.2",
    "eslint": "7.17.0",
    "eslint-config-google": "0.14.0",
    "eslint-config-prettier": "7.0.0",
    "eslint-config-standard": "16.0.2",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "4.0.2",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.1.3",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-snakecasejs": "2.2.0",
    "eslint-plugin-standard": "5.0.0",
    "eslint-plugin-vue": "7.4.0",
    "flush-promises": "^1.0.2",
    "font-awesome": "~4.7.0",
    "identity-obj-proxy": "~3.0.0",
    "jest": "~26.6.3",
    "jest-html-reporter": "~3.3.0",
    "jest-serializer-vue": "~2.0.2",
    "jest-transform-css": "~2.1.0",
    "jest-transform-stub": "~2.0.0",
    "jscpd": "^3.3.19",
    "jsdoc": "^3.6.6",
    "jsdoc-vuejs": "^3.0.8",
    "npm-merge-driver": "~2.3.6",
    "nuxt": "~2.14.4",
    "popper.js": "~1.16.0",
    "sinon": "^9.0.2",
    "testcafe": "^1.9.3",
    "vue-jest": "~3.0.5",
    "vue-template-compiler": "^2.6.11",
    "wait-for-expect": "~3.0.2"
  },
  "dependenciesComments": {
    "nuxt": "This is file defines all dependencies to widget library"
  },
  "license": "MIT"
}
