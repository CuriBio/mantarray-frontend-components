{
  "name": "@curi-bio/mantarray-frontend-components",
  "version": "1.6.3",
  "description": "shared components between web and desktop app.",
  "main": "./dist/mantarray.common.js",
  "author": "Curi Bio <contact@curibio.com> (http://www.curibio.com/)",
  "repository": {
    "type": "git",
    "url": "https://github.com/CuriBio/mantarray-frontend-components.git"
  },
  "scripts": {
    "dev": "check-engines && cross-env-shell NODE_ENV=development nuxt dev --spa",
    "build-dev": "check-engines && npm run build && cross-env-shell NODE_ENV=development nuxt dev --spa",
    "build": "check-engines && vue-cli-service build --target lib --name mantarray ./index.js",
    "test:unit-no-prebuild": "check-engines && jest -c jest.config.js",
    "test:unit": "npm run build && jest -c jest.config.js",
    "test:e2e": "npm run build && npm run _test:e2e -- --app 'npm run dev' --app-init-delay 35000",
    "test:e2e-parallel-no-prebuild": "npm run _test:e2e-parallel -- --app 'npm run dev' --app-init-delay 35000",
    "test:e2e-no-prebuild": "check-engines && npm run _test:e2e",
    "test:e2e-no-prebuild-github": "check-engines && npm run _test:e2e -- --app 'npm run dev' --app-init-delay 45000",
    "test:e2e-no-prebuild-windows-github": "npm run _test:e2e",
    "_test:e2e": "testcafe \"chrome:headless:emulation:width=1920;height=930;cdpPort=9223;\" ./tests/e2e --screenshots path=./tests/e2e/screenshots",
    "_test:e2e-parallel": "testcafe -c 2 \"chrome:headless:emulation:width=1920;height=930;cdpPort=9223;\" ./tests/e2e --screenshots path=./tests/e2e/screenshots",
    "prepare": "pip install -r requirements-dev.txt && pre-commit install && npx npm-merge-driver install",
    "nuxt-start": "check-engines && nuxt start",
    "lint": "eslint .",
    "docs": "jsdoc -c jsdoc.json"
  },
  "engines": {
    "node": "^16.17.0"
  },
  "files": [
    "dist/*",
    "*.json"
  ],
  "dependencies": {
    "@fortawesome/vue-fontawesome": "^2.0.0",
    "@tofandel/uuid-base62": "^1.0.3",
    "axios": "^0.26.0",
    "bootstrap-vue": "^2.21.1",
    "colorjs.io": "^0.0.3",
    "d3": "^6.2.0",
    "jquery": "^3.5.1",
    "semver-sort": "^1.0.0",
    "socket.io-client": "^4.1.3",
    "typeface-anonymous-pro": "^1.1.13",
    "typeface-muli": "^1.1.3",
    "vue-axios": "^3.2.2",
    "vue-fontawesome-icon": "^1.3.0",
    "vue-uuid": "^2.0.2",
    "vuedraggable": "^2.24.3",
    "vuex": "^3.5.1"
  },
  "devDependencies": {
    "@curi-bio/frontend-test-utils": "~0.2.3",
    "@fortawesome/fontawesome-free": "~6.1.1",
    "@fortawesome/fontawesome-svg-core": "~1.2.27",
    "@fortawesome/free-brands-svg-icons": "~5.15.1",
    "@fortawesome/free-regular-svg-icons": "~5.15.1",
    "@fortawesome/free-solid-svg-icons": "~5.15.1",
    "@vue/cli-service": "^4.4.6",
    "@vue/test-utils": "^1.1.1",
    "axios-mock-adapter": "^1.18.1",
    "core-js": "~3.10.0",
    "eslint": "~7.24.0",
    "eslint-config-google": "~0.14.0",
    "eslint-config-prettier": "~8.1.0",
    "eslint-config-standard": "~16.0.2",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-plugin-import": "~2.22.1",
    "eslint-plugin-jest": "~24.3.1",
    "eslint-plugin-node": "~11.1.0",
    "eslint-plugin-promise": "~4.3.1",
    "eslint-plugin-snakecasejs": "~2.2.0",
    "eslint-plugin-vue": "^8.4.1",
    "flush-promises": "^1.0.2",
    "identity-obj-proxy": "~3.0.0",
    "jest": "~26.6.3",
    "jest-html-reporter": "~3.3.0",
    "jest-serializer-vue": "~2.0.2",
    "jest-transform-css": "~2.1.0",
    "jest-transform-stub": "~2.0.0",
    "jscpd": "^3.3.19",
    "jsdoc": "^3.6.6",
    "jsdoc-vuejs": "^3.0.8",
    "npm-merge-driver": "~2.3.6",
    "nuxt": "^2.14.12",
    "sinon": "^10.0.0",
    "socket.io": "^4.1.3",
    "testcafe": "^1.15.0",
    "vue-jest": "~4.0.0-rc.1",
    "vue-template-compiler": "^2.6.11",
    "wait-for-expect": "~3.0.2"
  },
  "license": "MIT"
}
